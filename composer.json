{
  "name": "bradietilley/faker-foodz",
  "description": "Faker for cat profile pics",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BradieTilley\\FakerFoodz\\": "src/"
    },
    "files": [
        "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Bradie Tilley",
      "homepage": "https://github.com/bradietilley"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "illuminate/support": "^10.0|^11.0",
    "bradietilley/faker-imagez": "dev-main"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.1.5",
    "laravel/pint": "^1.0",
    "pestphp/pest": "^2.0",
    "phpstan/phpstan": "^1.8"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
